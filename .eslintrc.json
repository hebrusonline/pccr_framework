{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["google", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  }
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "valid-jsdoc": 1,
    "capitalized-comments": 1
  }
}
